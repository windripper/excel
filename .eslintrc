{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jest/globals": true
    },
    "rules": {
        "object-curly-spacing": "off",
        "arrow-parens": "off",
        "comma-dangle": "off",
        "indent": ["error", 4],
        "require-jsdoc": "off",
        "linebreak-style": "off",
        "operator-linebreak": "off",
        "no-debugger": "warn",
        "keyword-spacing": "off"
    },
    "plugins": ["jest"],
    "extends": [
        "eslint:recommended",
        "google"
    ]
}
